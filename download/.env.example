# Database Configuration
DATABASE_URL=postgresql://postgres:password@localhost:5432/notes_vibes_db

# Session Secret (use a strong random string in production)
SESSION_SECRET=notes-vibes-local-dev-secret

# Spotify API (required for music integration)
SPOTIFY_CLIENT_ID=your_spotify_client_id
SPOTIFY_CLIENT_SECRET=your_spotify_client_secret

# Optional: Set environment to development
NODE_ENV=development